package com.nyt.mpt.form;

/**
 * This form bean is used for SOS Order information
 * 
 * @author Shishir.Srivastava
 * 
 */
public class SOSOrderForm {

	private boolean syncToSOS;

	private long sosOrderId;

	private boolean reservationCreated;

	private AgencyForm agencyForm;

	private AdvertiserForm advertiserForm;

	public boolean isSyncToSOS() {
		return syncToSOS;
	}

	public void setSyncToSOS(final boolean syncToSOS) {
		this.syncToSOS = syncToSOS;
	}

	public AgencyForm getAgencyForm() {
		return agencyForm;
	}

	public void setAgencyForm(final AgencyForm agencyForm) {
		this.agencyForm = agencyForm;
	}

	public AdvertiserForm getAdvertiserForm() {
		return advertiserForm;
	}

	public void setAdvertiserForm(final AdvertiserForm advertiserForm) {
		this.advertiserForm = advertiserForm;
	}

	public long getSosOrderId() {
		return sosOrderId;
	}

	public void setSosOrderId(final long sosOrderId) {
		this.sosOrderId = sosOrderId;
	}

	public boolean isReservationCreated() {
		return reservationCreated;
	}

	public void setReservationCreated(final boolean reservationCreated) {
		this.reservationCreated = reservationCreated;
	}
}
