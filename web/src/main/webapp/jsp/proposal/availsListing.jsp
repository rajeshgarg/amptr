<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form"%>
<%@ taglib prefix="spring" uri="http://www.springframework.org/tags"%>


<div id="availsListing" >

<table width="100%" cellpadding="2" cellspacing="2" >
	
	<tr>			
		<td width="50%"><label class="label-bold"><spring:message code="label.generic.avails"/></label></td>
		<td width="50%" id="available">${summary.available}</td>
	</tr>
	
	<tr>			
		<td width="50%"><label class="label-bold"><spring:message code="label.generic.capacity"/></label></td>
		<td width="50%" id="capacity">${summary.capacity}</td>
	</tr>
	
	<tr>
		<td width="50%"><label class="label-bold"><spring:message code="label.generic.startDate"/></label></td>
		<td width="50%" id="startDate">${summary.startDate}</td>
	</tr>
	<tr>	
		<td width="50%"><label class="label-bold"><spring:message code="label.generic.endDate"/></label></td>
		<td width="50%" id="endDate">${summary.endDate}</td>
	</tr>
	
	
	<tr>
		<td width="50%" ><label class="label-bold"><spring:message code="label.generic.unmetDemand"/></label></td>
		<td width="50%" id="unmetDemand">${summary.unmetDemand}</td>
	
	</tr>	
	
	<tr>	
		<td width="50%"><label class="label-bold"><spring:message code="label.generic.exceededForecastWindow"/></label></td>
		<td width="50%" id="exceededForecastWindow">${summary.exceededForecastWindow}</td>		
	</tr>
	
	<tr style="display:none;" >
		<td width="50%" ><label class="label-bold"></label></td>
		<td width="50%" id="availsDate">${availsPopulatedDate}</td>
		
	</tr>
			


</table>
</div>

