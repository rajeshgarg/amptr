package com.nyt.mpt.util;

/**
 * @author amandeep.singh
 * 
 */
public class KeyValuePairPojo {

	private Long id;
	private String name;

	public Long getId() {
		return id;
	}

	public KeyValuePairPojo(final Long id, final String name) {
		super();
		this.id = id;
		this.name = name;
	}

	public void setId(final Long id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(final String name) {
		this.name = name;
	}
}
