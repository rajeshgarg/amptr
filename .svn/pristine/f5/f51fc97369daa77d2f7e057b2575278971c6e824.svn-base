package com.nyt.mpt.domain;

import com.thoughtworks.xstream.annotations.XStreamAlias;

/**
 * Used to capture product details from yield-ex inventory
 * @author manish.kesarwani
 *
 */
@XStreamAlias("productDetail")
public class ProductDetail {
	
	private boolean active;
	private boolean exists;
	private String externalId;
	private String productClass;
	private String productExpression;
//	private String productId;
	private String productMD5;
	private String productName;
	private String rateCard;
	private boolean reporting;
	private String safeProductExpression;
	private boolean sold;	
	private String resolved;
	
	public boolean isActive() {
		return active;
	}
	public void setActive(boolean active) {
		this.active = active;
	}
	public boolean isExists() {
		return exists;
	}
	public void setExists(boolean exists) {
		this.exists = exists;
	}
	public String getRateCard() {
		return rateCard;
	}
	public void setRateCard(String rateCard) {
		this.rateCard = rateCard;
	}
	public String getExternalId() {
		return externalId;
	}
	public void setExternalId(String externalId) {
		this.externalId = externalId;
	}
	public String getProductClass() {
		return productClass;
	}
	public void setProductClass(String productClass) {
		this.productClass = productClass;
	}
	public String getProductExpression() {
		return productExpression;
	}
	public void setProductExpression(String productExpression) {
		this.productExpression = productExpression;
	}
	/*public String getProductId() {
		return productId;
	}
	public void setProductId(String productId) {
		this.productId = productId;
	}*/
	public String getProductMD5() {
		return productMD5;
	}
	public void setProductMD5(String productMD5) {
		this.productMD5 = productMD5;
	}
	public String getProductName() {
		return productName;
	}
	public void setProductName(String productName) {
		this.productName = productName;
	}	
	public boolean isReporting() {
		return reporting;
	}
	public void setReporting(boolean reporting) {
		this.reporting = reporting;
	}
	public String getSafeProductExpression() {
		return safeProductExpression;
	}
	public void setSafeProductExpression(String safeProductExpression) {
		this.safeProductExpression = safeProductExpression;
	}
	public boolean isSold() {
		return sold;
	}
	public void setSold(boolean sold) {
		this.sold = sold;
	}
	public String getResolved() {
		return resolved;
	}
	public void setResolved(String resolved) {
		this.resolved = resolved;
	}	
}
