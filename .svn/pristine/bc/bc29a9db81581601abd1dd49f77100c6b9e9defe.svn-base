.graybutton{
  border: 1px solid #3079ed;  color: #fff;  text-shadow: 0 1px rgba(0,0,0,0.1);  background-color: #a1c4fe;  background-image: -webkit-gradient(linear,left top,left bottom,from(#a1c4fe),to(#6e99df));  background-image: -webkit-linear-gradient(top,#a1c4fe,#6e99df);  background-image: -moz-linear-gradient(top,#a1c4fe,#6e99df);  background-image: -ms-linear-gradient(top,#a1c4fe,#6e99df);  background-image: -o-linear-gradient(top,#a1c4fe,#6e99df);  background-image: linear-gradient(top,#a1c4fe,#6e99df);  font-size:12px;  padding:2px 7px; padding:3px 10px\0/IE8+9;  cursor:pointer;
 -moz-border-radius: 3px;	-khtml-border-radius: 3px;	-webkit-border-radius: 3px;	border-radius: 3px;		}

.graybutton:hover{
  border: 1px solid #3079ed;  color: #fff;  text-shadow: 0 1px rgba(0,0,0,0.1);  background-color: #789ad2;  background-image: -webkit-gradient(linear,left top,left bottom,from(#789ad2),to(#6e99df));  background-image: -webkit-linear-gradient(top,#789ad2,#6e99df);  background-image: -moz-linear-gradient(top,#789ad2,#6e99df);  background-image: -ms-linear-gradient(top,#789ad2,#6e99df);  background-image: -o-linear-gradient(top,#789ad2,#6e99df);  background-image: linear-gradient(top,#789ad2,#6e99df); 
 -moz-border-radius: 3px;	-khtml-border-radius: 3px;	-webkit-border-radius: 3px;	border-radius: 3px;		}

.buttonGeneric {  border: 1px solid #3079ed;  color: #fff;  text-shadow: 0 1px rgba(0,0,0,0.1);  background-color: #4d90fe;  background-image: -webkit-gradient(linear,left top,left bottom,from(#4d90fe),to(#4787ed));  background-image: -webkit-linear-gradient(top,#4d90fe,#4787ed);  background-image: -moz-linear-gradient(top,#4d90fe,#4787ed);  background-image: -ms-linear-gradient(top,#4d90fe,#4787ed);  background-image: -o-linear-gradient(top,#4d90fe,#4787ed);  background-image: linear-gradient(top,#4d90fe,#4787ed);  font-size:12px;  padding:2px 7px;padding:3px 10px\0/IE8+9;  cursor:pointer;
  -moz-border-radius: 3px;	-khtml-border-radius: 3px;	-webkit-border-radius: 3px;	border-radius: 3px;
}

.buttonGeneric:hover {  border: 1px solid #2f5bb7;
  color: #fff;
  text-shadow: 0 1px rgba(0,0,0,0.3);
  background-color: #357ae8;
  background-image: -webkit-gradient(linear,left top,left bottom,from(#4d90fe),to(#1659c5));
  background-image: -webkit-linear-gradient(top,#4d90fe,#1659c5);
  background-image: -moz-linear-gradient(top,#4d90fe,#1659c5);
  background-image: -ms-linear-gradient(top,#4d90fe,#1659c5);
  background-image: -o-linear-gradient(top,#4d90fe,#1659c5);
  background-image: linear-gradient(top,#4d90fe,#1659c5);
  cursor:pointer;
}

.buttonGeneric:active {
  -webkit-box-shadow: inset 0 1px 2px rgba(0,0,0,0.3);
  -moz-box-shadow: inset 0 1px 2px rgba(0,0,0,0.3);
  box-shadow: inset 0 1px 2px rgba(0,0,0,0.3);
  cursor:pointer;
}




