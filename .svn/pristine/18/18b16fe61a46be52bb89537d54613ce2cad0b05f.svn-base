package com.nyt.mpt.template;

import java.util.HashMap;
import java.util.Map;

import com.nyt.mpt.domain.Product;
import com.nyt.mpt.domain.SalesTarget;

/**
 * This class is used to populate reference data like product, sales target from SOS
 * @author manish.kesarwani
 * 
 *
 */
public class ReferenceDataMap {

	private Map<Long, Product> productMap = new HashMap<Long, Product>();

	private Map<Long, SalesTarget> salesTargetMap = new HashMap<Long, SalesTarget>();

	private Map<Long, String> targetType = new HashMap<Long, String>();

	public Map<Long, Product> getProductMap() {
		return productMap;
	}

	public void setProductMap(Map<Long, Product> productMap) {
		this.productMap = productMap;
	}

	public Map<Long, SalesTarget> getSalesTargetMap() {
		return salesTargetMap;
	}

	public void setSalesTargetMap(HashMap<Long, SalesTarget> salesTargetMap) {
		this.salesTargetMap = salesTargetMap;
	}

	public Map<Long, String> getTargetType() {
		return targetType;
	}

	public void setTargetType(Map<Long, String> targetType) {
		this.targetType = targetType;
	}

}
