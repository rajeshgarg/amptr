package com.nyt.mpt.util.reservation;

import java.util.ArrayList;
import java.util.List;

import com.nyt.mpt.domain.Proposal;
import com.nyt.mpt.domain.sos.SalesOrder;

/**
 * This <code>ReservationDetails</code> contains attributes for getting more details of Reservation 
 * 
 * @author surendra.singh
 */
public class ReservationDetails {

	private Double totalSOR;
	
	private List<SalesOrder> salesOrderList = new ArrayList<SalesOrder>();
	
	private List<Proposal> proposalList = new ArrayList<Proposal>();

	public Double getTotalSOR() {
		return totalSOR;
	}

	public void setTotalSOR(Double totalSOR) {
		this.totalSOR = totalSOR;
	}

	public List<SalesOrder> getSalesOrderList() {
		return salesOrderList;
	}

	public void setSalesOrderList(List<SalesOrder> salesOrderList) {
		this.salesOrderList = salesOrderList;
	}

	public List<Proposal> getProposalList() {
		return proposalList;
	}

	public void setProposalList(List<Proposal> proposalList) {
		this.proposalList = proposalList;
	}
}
